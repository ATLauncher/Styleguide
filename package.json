{
  "name": "@atlauncher/next",
  "version": "0.0.0",
  "description": "Monorepo of different packages used by JavaScript/TypeScript based projects at ATLauncher.",
  "private": true,
  "homepage": "https://github.com/ATLauncher/next#readme",
  "bugs": {
    "url": "https://github.com/ATLauncher/next/issues"
  },
  "license": "MIT",
  "author": {
    "name": "Ryan Dowling",
    "email": "ryan.dowling@atlauncher.com",
    "url": "https://github.com/RyanTheAllmighty"
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/ATLauncher/next.git"
  },
  "devDependencies": {
    "husky": "~0.14.3",
    "lerna": "~2.8.0"
  },
  "scripts": {
    "commitmsg": "lerna run --scope @atlauncher/atlauncher-scripts commitmsg",
    "postinstall": "lerna bootstrap",
    "publish": "lerna publish --conventional-commits --exact -m \"chore: publish\"",
    "publish:prerelease":
      "lerna publish --conventional-commits --exact --cd-version=prerelease -m \"chore: publish prerelease\""
  },
  "engineStrict": true,
  "engines": {
    "node": ">=8",
    "npm": ">=5"
  }
}
